  {
    "name": "node-keyboard-watcher",
    "version": "1.0.3",
    "author": {
      "name": "Guus van der Meer"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/OhMyGuus/node-keyboard-watcher.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
  "private": true,
  "gypfile": true,
  "scripts": {
    "install": "node-gyp rebuild --openssl_fips='' && yarn build-ts",
    "build32": "node-gyp clean configure build --arch=ia32 --openssl_fips=''",
    "build64": "node-gyp clean configure build --arch=x64 --openssl_fips=''",
    "prebuild": "prebuildify --napi",
    "build-ts": "tsc",
    "demo": "node-gyp build --openssl_fips='' && yarn build-ts && node dist/demo/keyboardWatcher-demo.js"
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "binding.gyp",
    "src/lib",
    "prebuilds"
  ],
  "devDependencies": {
    "@types/node": "12.x.x",
    "prebuildify": "4.x.x",
    "ts-node": "9.x.x",
    "typescript": "4.x.x"
  },
  "dependencies": {
    "bindings": "*",
    "node-addon-api": "*",
    "@types/throttle-debounce": "2.x.x",
    "throttle-debounce": "2.x.x",
    "ts-node": "9.x.x",
    "typescript": "4.x.x"
  }
  
}
